{
    "id": "etcd",
    "container": {
        "docker": {
            "image": "mesosphere/etcd-mesos:latest",
            "forcePullImage": true
        },
        "type": "DOCKER",
    },
    "cpus": 0.2,
    "mem": 128.0,
    "instances": 1,
    "portDefinitions": [0, 0, 0],
    "env": {
      "FRAMEWORK_NAME":"etcd",
      "FRAMEWORK_FAILOVER_TIMEOUT_SECONDS":"300",
      "WEBURI": "http://etcd.marathon.mesos:$PORT0/stats",
      "CLUSTER_SIZE":"3",
      "MESOS_MASTER":"zk://localhost:2181/mesos",
      "ZK_PERSIST":"zk://localhost:2181/etcd-mesos",
      "VERBOSITY":"1",
      "SINGLE_INSTANCE_PER_SLAVE":"true",
      "AUTO_RESEED":"true",
      "RESEED_TIMEOUT":"240",
      "DISK_LIMIT":"4096",
      "CPU_LIMIT":"1",
      "MEM_LIMIT":"2048"
    }
}
